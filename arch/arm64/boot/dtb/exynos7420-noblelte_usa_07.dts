 wacom_pwr_en {
  compatible = "regulator-fixed";
  regulator-name = "wacom_3.3v";
  regulator-min-microvolt = <3300000>;
  regulator-max-microvolt = <3300000>;
  gpio = <&gpd1 5 0>;
  enable-active-high;
  regulator-boot-on;
 };

 hsi2c@14E60000 {
  status = "okay";
  samsung,hs-mode;
  clock-frequency = <1000000>;
  earSmart@3e {
   compatible = "earSmart";
   reg = <0x3e>;
   clocks = <&clock 45>;
   clock-names = "mclk";
   pinctrl-0 = <&es704_i2c &es704_reset &es704_wakeup>;
   esxxx-reset-gpio = <&gpf1 0 0>;
   esxxx-wakeup-gpio = <&gpd0 1 0>;
   adnc,firmware_name = "audience-es804-fw.bin";
   adnc,use_dhwpt;
  };
 };

 i2c@20{
  touchkey@20 {
   compatible = "abov,mc96ft16xx";
   status = "disabled";
  };
  touchkey@20 {
   compatible = "cypress,cypress_touchkey";
   status = "okay";
   pinctrl-names = "on_irq","off_irq","sub_det";
   pinctrl-0 = <&cfg_irq>;
   pinctrl-1 = <&cfg_input>;
   pinctrl-2 = <&sub_det>;
   reg = <0x20>;
   interrupts = <0 0 0>;
   interrupt-parent = <&gpa0>;
   cypress,sda-gpio = <&gpg1 2 0>;
   cypress,scl-gpio = <&gpg1 3 0>;
   cypress,irq-gpio = <&gpa0 0 1>;
   cypress,sub-det = <&gpa3 0 0>;
   cypress,ap-io-power = "true";
   cypress,i2c-gpio = "true";
   cypress,ic-stabilizing-time = <150>;
   cypress,boot-on-ldo = "true";
   cypress,one_keypad = "true";
   cypress,fw_path = "cypress/cypress_noble.fw";
  };
 };

 serial_0: uart@13630000 {
  status = "okay";
  samsung,use-default-irq;
 };

 dwmmc0@15740000 {
  status = "disabled";
 };

 dwmmc2@15560000 {
  vdd_vmmc-supply = <&ldo25_reg>;
  vqmmc-supply = <&ldo2_reg>;
 };

 simslot_count {
  status = "okay";
 };
};
